export PATH="$HOME/.yarn/bin:$HOME/.config/yarn/global/node_modules/.bin:$PATH"

# use homebrew ssl for ruby
export RUBY_CONFIGURE_OPTS="--with-openssl-dir=$(brew --prefix openssl@1.1)"

export ANDROID_HOME=/Users/$USER/Library/Android/sdk
path+=('/Users/jakobkoschel/Library/Android/sdk/platform-tools/')

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/jakobkoschel/Developer/google-cloud-sdk/path.zsh.inc' ]; then source '/Users/jakobkoschel/Developer/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/jakobkoschel/Developer/google-cloud-sdk/completion.zsh.inc' ]; then source '/Users/jakobkoschel/Developer/google-cloud-sdk/completion.zsh.inc'; fi

export NVM_DIR="$HOME/.nvm"
[ -s "/usr/local/opt/nvm/nvm.sh" ] && . "/usr/local/opt/nvm/nvm.sh"  # This loads nvm
[ -s "/usr/local/opt/nvm/etc/bash_completion.d/nvm" ] && . "/usr/local/opt/nvm/etc/bash_completion.d/nvm"  # This loads nvm bash_completion

export PATH="/usr/local/opt/gradle@2.14/bin:$PATH"

export COCOAPODS_DISABLE_STATS=true

export PATH="$HOME/.yarn/bin:$HOME/.config/yarn/global/node_modules/.bin:$PATH"

export PATH="$PATH:/Users/jakobkoschel/Developer/flutter/bin"

export PATH="/usr/local/lib/ruby/gems/2.7.0/bin:$PATH"
export PATH="/usr/local/opt/ruby/bin:$PATH"

export VIRTUALENVWRAPPER_PYTHON=$(which python3)

source /usr/local/bin/virtualenvwrapper.sh

export PATH="$HOME/.rbenv/shims:$PATH"
